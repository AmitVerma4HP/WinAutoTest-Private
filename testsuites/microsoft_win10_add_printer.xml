<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Microsoft Win10 Test suite - Add Printer In Settings">
	<parameter name="device_name" value="DESKTOP-46461K1" />
 

<!-- If Printer under tests is already present or added previously then this test will remove that printer, will ensure printer is no longer added and  -->
<!-- then go for discovery then Add and validate that printer is added successfully then mark test as PASS --> 
 
 
<!-- **************************************** START: ADD PRINTER TESTS ******************************************************** -->
  
  	<test verbose="2" name="Test-01 : Add Printer Tests from Windows Settings - Printer1">    	
     	<parameter name="ptr_name" value="ipp: Canon TS6000 series" /> 		
