<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Microsoft Win10 MSWord 2016 Test suite - Printing Tests">
 
  <!-- ********************************************** PRINTING MSWORD TEST FILE *********************************************************** -->
  
	 <test verbose="2" name="Printing Test MSWord test file - Device Under test (VERAMIT6) => Printer Under Test - HPBFB476 (HP Officejet Pro 8600)">
    	<parameter name="device_name" value="VERAMIT6" />
     	<parameter name="ptr_name" value="HPBFB476 (HP Officejet Pro 8600)" />
     	<parameter name="paper_size" value="Legal" />
     	<parameter name="duplex_option" value="Print One Sided" />
     	<parameter name="orientation" value="Portrait Orientation" />
     	<parameter name="collation" value="Collated" />
     	<parameter name="copies" value="2" />
     	<parameter name="pages_to_print" value="Print All Pages" />  
<!-- 		<parameter name="page_count" value="1,3-4" />      	 -->
     	<parameter name="test_filename" value="MicrosoftWord2016_Portrait_MultiPage_TestFile.docx" />
		
    <classes>
         <class name="com.hp.win.tests.PrintMsWord"/>
    </classes>
  </test>
 

	 <test verbose="2" name="Printing Test MSWord test file2 - Device Under test (VERAMIT6) => Printer Under Test - HPBFB476 (HP Officejet Pro 8600)">
