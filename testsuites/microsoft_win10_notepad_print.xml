<!-- ************************* OJ 8600 on R12 ************************* -->
    <test verbose="2" name="Notepad Duplex Short Edge Color - Device Under test (LAPTOP-3KFJRU7N) => Printer Under Test (ipp: Officejet Pro 8600 [B80E15])">
    	<parameter name="device_name" value="LAPTOP-3KFJRU7N" />
     	<parameter name="ptr_name" value="ipp: Officejet Pro 8600 [B80E15]" />
     	<parameter name="test_filename" value="NotepadTestFile1.txt" />
