     	<parameter name="duplex_optn" value="Short Edge" />
     	<parameter name="color_optn" value="Color" />
     	<parameter name="paper_size" value="Letter" />
		
    <classes>
         <class name="com.hp.win.tests.PrintFromNotepad"/>
    </classes>
  </test>
 
  
